{
  "name": "react-admin-template-smart",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "",
  "scripts": {
    "start": "run-s create-config:local vite-start",
    "vite-start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "lint": "run-s lint:scss lint:ts",
    "lint:ts": "eslint --no-error-on-unmatched-pattern ./src/**/*.{ts,tsx}",
    "lint:ts:fix": "eslint --no-error-on-unmatched-pattern ./src/**/*.{ts,tsx} --fix",
    "lint:scss": "npx stylelint ./src/**/*.scss",
    "lint:css:fix": "npx stylelint ./src/**/*.{css,scss} --fix",
    "cypress:open": "cypress open --e2e --browser chrome",
    "cypress:open:component": "cypress open --component --browser chrome",
    "cypress:run": "cypress run",
    "create-config:local": "node local-config-builder local",
    "prepare": "husky install",
    "check-commit": "commitlint --edit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.20.2",
    "axios": "^1.6.7",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.22.0",
    "ts-node": "^10.9.2",
    "vite": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.10",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "^4.0.9",
    "sass": "^1.70.0",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
